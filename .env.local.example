# Local Setup ke liye .env file
# Is file ko copy karke .env naam se save karein

# Telegram Bot Token (BotFather se milega)
BOT_TOKEN=your_bot_token_here

# FIREx Website Credentials
FIREX_USERNAME=your_username_here
FIREX_PASSWORD=your_password_here

# Flask Port
PORT=5000

# Optional: Proxy (agar IP block ho raha hai)
# PROXY_URL=http://username:password@proxy-server:port
